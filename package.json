{
  "name": "electron-template",
  "version": "1.0.0",
  "description": "electron启动脚手架「electron+forge+vue3」",
  "main": "build/src/main.js",
  "author": "wl",
  "homepage": "https://gitee.com/missmess",
  "scripts": {
    "start": "tsc && cross-env NODE_ENV=development electron-forge start",
    "package": "tsc && cross-env NODE_ENV=production electron-forge package",
    "make": "tsc && cross-env NODE_ENV=production electron-forge make",
    "make-win": "tsc && cross-env NODE_ENV=production electron-forge make --platform=win32 --arch=x64",
    "webDev": "cd webapps/vue3-webapp && npm run dev",
    "webBuild": "cd webapps/vue3-webapp && npm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "cross-env": "^7.0.3",
    "electron": "14.0.0",
    "enquirer": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
